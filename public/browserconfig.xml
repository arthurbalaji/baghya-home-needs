<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
    <msapplication>
        <tile>
            <square70x70logo src="/favicon-96x96.png"/>
            <square150x150logo src="/web-app-manifest-192x192.png"/>
            <square310x310logo src="/web-app-manifest-512x512.png"/>
            <TileColor>#ec4899</TileColor>
        </tile>
        <notification>
            <polling-uri src="/notifications/contoso1.xml"/>
            <polling-uri2 src="/notifications/contoso2.xml"/>
            <polling-uri3 src="/notifications/contoso3.xml"/>
            <polling-uri4 src="/notifications/contoso4.xml"/>
            <polling-uri5 src="/notifications/contoso5.xml"/>
            <frequency>30</frequency>
            <cycle>1</cycle>
        </notification>
    </msapplication>
</browserconfig>